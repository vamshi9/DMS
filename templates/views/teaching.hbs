<div class="container-fluid teaching">
    <div class="row">
        {{#each data.teachingData}}
            <div class="col s12 m8 offset-m2 l5">
                <div class="card-panel grey lighten-5 z-depth-1">
                    <div class="row valign-wrapper">
                        <div class="col s2">
                            <img src='{{user.profilePhoto}}' alt="" class="circle responsive-img">
                        </div>
                        <div class="col s10">
                            
                                <blockquote>
                                    This is a square image. Add the "circle" class to it to make it appear circular.
                                </blockquote>
                            
                        </div>
                    </div>
                </div>
            </div>
            <div class="col s12 m8 offset-m2 l5 offset-l7">
                <div class="card-panel grey lighten-5 z-depth-1">
                    <div class="row valign-wrapper">
                        <div class="col s10">
                            <span class="black-text">
                                This is a square image. Add the "circle" class to it to make it appear circular.
                            </span>
                        </div>
                        <div class="col s2">
                            <img src="images/prof.sridhar.jpg" alt="" class="circle responsive-img">
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>
